{
  "name": "progress-central",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "watch": "nodemon server.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      [
        "babel-root-import"
      ]
    ]
  },
  "dependencies": {
    "assert": "^1.4.0",
    "aws-sdk": "^2.3.18",
    "bcrypt": "^0.8.6",
    "body-parser": "~1.13.2",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "~1.3.5",
    "cron": "^1.1.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "express-validator": "^2.20.4",
    "fbemitter": "^2.0.2",
    "flux": "^2.1.1",
    "formidable": "^1.0.17",
    "formsy-react": "^0.18.0",
    "gm": "^1.22.0",
    "jade": "~1.11.0",
    "jquery": "^2.2.3",
    "mailcomposer": "^3.9.0",
    "mailgun-js": "^0.7.11",
    "mime": "^1.3.4",
    "moment": "^2.13.0",
    "mongoose": "^5.7.7",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-loader": "^2.4.0",
    "react-router": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3",
    "validator": "^5.2.0"
  },
  "jshintConfig": {
    "asi": true,
    "esnext": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true,
      "modules": true
    }
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-root-import": "^3.2.2",
    "chai": "^3.5.0",
    "errorhandler": "^1.4.3",
    "jest-cli": "^12.1.1",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.1.0",
    "superagent": "^1.8.3",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
